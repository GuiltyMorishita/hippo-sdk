{
  "name": "@manahippo/hippo-sdk",
  "version": "0.10.3",
  "description": "Hippo SDK for Aptos",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "typings": "dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "repository": "git@hippo.github.com:hippospace/hippo-sdk.git",
  "author": "mana <mana@hippo.space>",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist; rollup -c rollup.config.js",
    "build-cli": "rm -rf dist; rollup -c rollup.config.cli.js",
    "build-tsc": "rm -rf dist; tsc -p tsconfig.json",
    "cli-ts": "ts-node src/tools/index.ts agg list-quotes devBTC devUSDC 1 -c ../.aptos/config.yaml",
    "cli": "node cli/index.js",
    "test": "jest"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.35",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.1",
    "prettier": "^2.6.2",
    "rollup": "^2.79.0",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@manahippo/move-to-ts": "0.4.3",
    "aptos": "1.3.15",
    "commander": "^9.3.0",
    "yaml": "^2.1.1"
  }
}
